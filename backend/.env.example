# Python backend environment variables
# Copy this to .env and fill in your values

# Supabase Postgres (used by SQLAlchemy async engine)
# Get this from Supabase dashboard → Project Settings → Database → Connection string (URI)
DATABASE_URL=postgresql://postgres:<password>@db.<project-ref>.supabase.co:5432/postgres

# FastAPI server
PYTHON_API_PORT=8000

# Set to "true" to enable SQLAlchemy query logging
DB_ECHO=false
